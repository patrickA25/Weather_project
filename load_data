--load 2011
BULK INSERT [weather_DB].[dbo].[MAIN_TABLE]
    FROM 'E:/Weather_data/3_read_to_load/2011_upload_ready.csv'
    WITH
    (
	KEEPNULLS,
	firstrow = 2,
    FIELDTERMINATOR = '|',  --CSV field delimiter
    ROWTERMINATOR = '0x0a'   --Use to shift the control to next row
    )
go
